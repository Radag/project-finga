{block content}
<div id="contentWrapper">
    <h2>Nový obrázek do pravého sloupce</h2>
    
    {form newRightImageForm}
    <ul class="errors" n:if="$form->hasErrors()">
            <li n:foreach="$form->errors as $error">{$error}</li>
    </ul>

    <table>
        <tr>
            <td>{label image /}</td>
            <td>{input image}</td>

        </tr>
        <tr>
            <td></td>
            <td style="text-align: right;">{input send}</td>
        </tr>
    </table>
    {/form}
</div>